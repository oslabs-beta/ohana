{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["airbnb", "prettier"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "rules": {
      "prettier/prettier": ["error", { "singleQuote": true }],
      "react/jsx-filename-extension": "off",
      "import/extensions": "off"
    }
  }
  
